{
    "compilerOptions": {
        "target": "ES2022",
        "lib": [
            "ES2022",
            "DOM"
        ],
        "module": "ESNext",
        "moduleResolution": "Bundler",
        "esModuleInterop": true,
        "strict": true,
        "jsx": "react-jsx",
        "allowJs": false,
        "forceConsistentCasingInFileNames": true,
        "skipLibCheck": true,
        "types": [
            "node"
        ]
    },
    "include": [
        "app",
        "agents",
        "lib",
        "next-env.d.ts"
    ],
    "exclude": [
        "node_modules"
    ]
}