{
    "compilerOptions": {
        "target": "ES2022",
        "lib": [
            "ES2022",
            "DOM"
        ],
        "module": "ESNext",
        "moduleResolution": "Bundler",
        "esModuleInterop": true,
        "strict": true,
        "jsx": "react-jsx",
        "skipLibCheck": true,
        "types": [
            "node"
        ],
        "baseUrl": ".",
        "paths": {
            "@/*": [
                "*"
            ]
        }
    }
}